                        <td class="column100 column4 cell" data-column="column4" data-day="Wednesday" data-meal="5">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column5 cell" data-column="column5" data-day="Thursday" data-meal="5">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column6 cell" data-column="column6" data-day="Friday" data-meal="5">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column7 cell" data-column="column7" data-day="Saturday" data-meal="5">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column8 cell" data-column="column8" data-day="Sunday" data-meal="5">
                            {{> meal-name}}
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </main>
    <script src="/js/renderMeals.js"></script>

</body>