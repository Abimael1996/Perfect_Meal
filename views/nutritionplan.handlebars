                            {{> meal-name}}

                        </td>
                        <td class="column100 column6 cell" data-column="column6" data-day="Friday" data-meal="4">
                            {{> meal-name}}


                        </td>
                        <td class="column100 column7 cell" data-column="column7" data-day="Saturday" data-meal="4">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column8 cell" data-column="column8" data-day="Sunday" data-meal="4">
                            {{> meal-name}}

                        </td>
                    </tr>
                    <tr class="row100" id="5">
                        <td class="column100 column1" data-column="column1">Meal 3</td>

                        </td>
                        <td class="column100 column2 cell" data-column="column2" data-day="Monday" data-meal="5">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column3 cell" data-column="column3" data-day="Tuesday" data-meal="5">
                            {{> meal-name}}

