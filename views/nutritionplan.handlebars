<link rel="stylesheet" type="text/css" href="/css/nutritionplan.css">

<body>
    <main>
        <div class="text-center">
            <ul class="pagination justify-content-center">
                <li class="page-item disabled">
                    <a class="page-link" href="#" tabindex="-1">Previous</a>
                </li>
                <li class="page-item">
                    <p>{{name}}</p>
                </li>
                <li class="page-item">
                    <a class="page-link" href="#">Next</a>
                </li>
            </ul>
        </div>
        <div class="container">
            <table class="table table-bordered table-hover">
                <thead>
                    <tr class="row100 head">
                        <th class="column100 column1" data-column="column1"></th>
                        <th class="column100 column2" data-column="column2">Monday</th>
                        <th class="column100 column3" data-column="column3">Tuesday</th>
                        <th class="column100 column4" data-column="column4">Wednesday</th>
                        <th class="column100 column5" data-column="column5">Thursday</th>
                        <th class="column100 column6" data-column="column6">Friday</th>
                        <th class="column100 column7" data-column="column7">Saturday</th>
                        <th class="column100 column8" data-column="column8">Sunday</th>
                    </tr>
                </thead>
                <tbody>
                    <tr class="row100" id="1">
                        <td class="column100 column1" data-column="column1">Meal 1</td>
                        <td class="column100 column2 cell" data-column="column2" data-day="Monday" data-meal="1">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column3 cell" data-column="column3" data-day="Tuesday" data-meal="1">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column4 cell" data-column="column4" data-day="Wednesday" data-meal="1">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column5 cell" data-column="column5" data-day="Thursday" data-meal="1">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column6 cell" data-column="column6" data-day="Friday" data-meal="1">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column7 cell" data-column="column7" data-day="Saturday" data-meal="1">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column8 cell" data-column="column8" data-day="Sunday" data-meal="1">
                            {{> meal-name}}

                        </td>
                    </tr>
                    <tr class="row100" id="2">
                        <td class="column100 column1" data-column="column1">Snack</td>
                        <td class="column100 column2 cell" data-column="column2" data-day="Monday" data-meal="2">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column3 cell" data-column="column3" data-day="Tuesday" data-meal="2">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column4 cell" data-column="column4" data-day="Wednesday" data-meal="2">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column5 cell" data-column="column5" data-day="Thursday" data-meal="2">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column6 cell" data-column="column6" data-day="Friday" data-meal="2">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column7 cell" data-column="column7" data-day="Saturday" data-meal="2">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column8 cell" data-column="column8" data-day="Sunday" data-meal="2">
                            {{> meal-name}}

                        </td>
                    </tr>
                    <tr class="row100" id="3">
                        <td class="column100 column1" data-column="column1">Meal 2</td>
                        <td class="column100 column2 cell" data-column="column2" data-day="Monday" data-meal="3">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column3 cell" data-column="column3" data-day="Tuesday" data-meal="3">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column4 cell" data-column="column4" data-day="Wednesday" data-meal="3">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column5 cell" data-column="column5" data-day="Thursday" data-meal="3">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column6 cell" data-column="column6" data-day="Friday" data-meal="3">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column7 cell" data-column="column7" data-day="Saturday" data-meal="3">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column8 cell" data-column="column8" data-day="Sunday" data-meal="3">
                            {{> meal-name}}

                        </td>
                    </tr>
                    <tr class="row100" id="4">
                        <td class="column100 column1" data-column="column1">Snack</td>
                        <td class="column100 column2 cell" data-column="column2" data-day="Monday" data-meal="4">
                            {{> meal-name}}

                        </td>
                        <td class="column100 column3 cell" data-column="column3" data-day="Tuesday" data-meal="4">
                            {{> meal-name}}

